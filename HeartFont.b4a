Sub b1_Click
		If ph.SdkVersion > 19 Then
		ad1.Enabled = True
	End If
Msgbox("1. Install Font App" & CRLF & "2. After Click Change Font and Choose your Font!","Attention!")
	File.Copy(File.DirAssets,"Heart.apk",File.DirRootExternal,"Heart.apk")
	Dim i As Intent
    i.Initialize(i.ACTION_VIEW,"file:///"&File.DirRootExternal&"/Heart.apk")
	i.SetType("application/vnd.android.package-archive")
	StartActivity(i)
End Sub

Sub b2_Click
		If ph.SdkVersion > 19 Then
		ad1.Enabled = True
	End If
		ToastMessageShow("1. Click Font Style Or Font > " &CRLF& "2. Choose Myanmar Heart(iFont)",True)
	Dim abc As Intent
	abc.Initialize("android.settings.DISPLAY_SETTINGS","")
	StartActivity(abc)
End Sub

Sub b3_Click
		If ph.SdkVersion > 19 Then
		ad1.Enabled = True
	End If
	Msgbox("App Name : Myanmar Heart Font" & CRLF & "Version : 1.0" & CRLF & "Dev  By : Khun Htetz Naing" & CRLF&CRLF& "Special Thank to Font Dev!","About App")
End Sub

Sub Activity_Resume
If File.Exists(File.DirRootExternal,"Heart.apk") = True Then
	File.Delete(File.DirRootExternal,"Heart.apk")
End If
End Sub

Sub ad1_Tick
	If ph.SdkVersion > 19 Then
	If Interstitial.Status = Interstitial.Status_AdReadyToShow Then
		Interstitial.Show
	End If
	
	If Interstitial.Status = Interstitial.Status_Dismissed Then
		Interstitial.LoadAd
	End If
	End If
	ad1.Enabled = False
End Sub

Sub ad2_Tick
		If ph.SdkVersion > 19 Then
	If Interstitial.Status = Interstitial.Status_AdReadyToShow Then
		Interstitial.Show
	End If
	
	If Interstitial.Status = Interstitial.Status_Dismissed Then
		Interstitial.LoadAd
	End If
	End If
End Sub

Sub Activity_Pause (UserClosed As Boolean)

End Sub
